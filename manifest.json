{
  "manifest_version": 2,
  "name": "Facebook Feed Motivation",
  "description": "Extension to replace facebook feed by motivation message.",
  "version": "0.0.0.1",

  "permissions": [
    "activeTab",
    "http://*.facebook.com/",
    "https://*.facebook.com/"
  ],

  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  }

}
